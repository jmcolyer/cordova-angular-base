<?xml version='1.0' encoding='utf-8'?>
<widget id="com.rolyer.kitchensink" version="4.4.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>KitchenSinkApp</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
	<!-- Default application icon -->
	<icon src="www/assets/images/angular.png" />
	<!--
		Default icon should work, but cordova don't overwrite
		the default on all densities
	-->
	<icon src="www/assets/images/angular.png" platform="android" density="ldpi" />
	<icon src="www/assets/images/angular.png" platform="android" density="mdpi" />
	<icon src="www/assets/images/angular.png" platform="android" density="hdpi" />
	<icon src="www/assets/images/angular.png" platform="android" density="xhdpi" />	
</widget>
